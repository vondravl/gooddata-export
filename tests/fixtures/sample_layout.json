          "layout": {
            "sections": [
              {
                "items": [
                  {
                    "widget": {
                      "type": "insight",
                      "localIdentifier": "widget_revenue_trend",
                      "insight": {"identifier": {"id": "viz_revenue_trend", "type": "visualizationObject"}}
