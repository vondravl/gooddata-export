{
  "analytics": {
    "metrics": [
      {
        "id": "metric_total_revenue",
        "title": "Total Revenue",
        "description": "Sum of all revenue",
        "tags": ["finance", "kpi"],
        "content": {
          "maql": "SELECT SUM({fact/revenue})",
          "format": "#,##0.00"
        },
        "createdAt": "2024-01-15T10:00:00Z",
        "modifiedAt": "2024-06-01T14:30:00Z",
        "areRelationsValid": true,
        "isHidden": false
      },
      {
        "id": "metric_avg_order_value",
        "title": "Average Order Value",
        "description": "Revenue divided by order count",
        "tags": ["finance"],
        "content": {
          "maql": "SELECT {metric/metric_total_revenue} / COUNT({attr/order_id})",
          "format": "#,##0.00"
        },
        "createdAt": "2024-02-01T09:00:00Z",
        "modifiedAt": "2024-02-01T09:00:00Z",
        "areRelationsValid": true,
        "isHidden": false
      },
      {
        "id": "metric_order_count",
        "title": "Order Count",
        "description": "Total number of orders",
        "tags": ["operations"],
        "content": {
          "maql": "SELECT COUNT({attr/order_id})",
          "format": "#,##0"
        },
        "createdAt": "2024-01-10T08:00:00Z",
        "modifiedAt": "2024-03-15T11:00:00Z",
        "areRelationsValid": true,
        "isHidden": false
      }
    ],
    "visualizationObjects": [
